{
  "name": "free-saas-suite",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:gateway": "tsx watch src/gateway/index.ts",
    "dev:money-in": "tsx watch src/money-in/index.ts",
    "dev:money-out": "tsx watch src/money-out/index.ts",
    "dev:intelligence": "tsx watch src/intelligence/main.ts",
    "dev": "concurrently -n gateway,money-in,money-out,intelligence -c \"bgBlue.bold,bgGreen.bold,bgMagenta.bold,bgCyan.bold\" \"npm run dev:gateway\" \"npm run dev:money-in\" \"npm run dev:money-out\" \"npm run dev:intelligence\"",
    "build": "tsc",
    "start:gateway": "node dist/gateway/index.js",
    "start:money-in": "node dist/money-in/index.js",
    "start:money-out": "node dist/money-out/index.js",
    "start:intelligence": "node dist/intelligence/main.js",
    "start": "concurrently -n gateway,money-in,money-out,intelligence \"npm run start:gateway\" \"npm run start:money-in\" \"npm run start:money-out\" \"npm run start:intelligence\"",
    "migrate:gateway": "node --loader tsx src/gateway/db/migrations.ts",
    "migrate:money-in": "node --loader tsx src/money-in/db/migrations.ts",
    "migrate:money-out": "node --loader tsx src/money-out/db/migrations.ts",
    "migrate:intelligence": "node --loader tsx scripts/migrate-intelligence.ts",
    "seed:gateway": "node --loader tsx src/gateway/db/seed.ts",
    "jobs:money-in": "node --loader tsx src/money-in/jobs.ts",
    "jobs:money-out": "node --loader tsx src/money-out/jobs.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/multipart": "^9.0.0",
    "@fastify/sensible": "^6.0.2",
    "adm-zip": "^0.5.12",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastify": "^5.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.4",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.15.0",
    "pg": "^8.12.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.12.12",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.4",
    "@types/pg": "^8.11.5",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  }
}
